{
    "manifest_version": 3,
    "name": "LOL IUI Extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "./modal.css"
            ],
            "js": [
                "content.js",
                "TopPlayerPerChamp.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ]
}